{
    "$id": "file://batmo/schemas/electrolyte",
    "$schema": "http://json-schema.org/draft-07/schema#",
    "description": "A standard electrolyte",
    "type": "object",
    "properties" : {
        "electrolyteType" : {"type" : "string",
                             "description" : "type of electrolyte (binary or ...)"},
        "heatCapacity": {"type" : "number"},
        "thermalConductivity": {"type" : "number"}, 
        "chargeCarrierName": {"type" : "string"},
        "updateConductivityFunc": {"$ref" : "function",
                                   "description" : "function to update conductivity"},
        "updateDiffusionCoefficientFunc" : {"$ref" : "function",
                                            "description" : "function to update diffusion coefficient"},
        "compnames": {"type" : "array"},
        "name": {"type" : "string"},
        "sp": { "type" : "object",
                "properties" : {
                    "z": {"type" : "array"},
                    "t": {"type" : "array"}
                }
              },
        "Separator" : {"$ref" : "separator"},
        "negativeDomainBruggemanCoefficient" : {"type" : "number", ...
                                                "description": "Bruggeman coefficient for the electrolyte in the negative electrode domain"},
        "positiveDomainBruggemanCoefficient" : {"type" : "number", ...
                                                "description": "Bruggeman coefficient for the electrolyte in the positive electrode domain"}
    }
}
