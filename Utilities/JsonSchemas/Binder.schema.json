{
    "$id": "file://battmo/schemas/Binder",
    "$schema": "http://json-schema.org/draft-07/schema#",
    "description": "Binder",
    "type": "object",
  
    "allOf": [{"properties" : { "electronicConductivity": {
                                  "$ref": "#/definitions/PhysicalQuantity",
                                  "description": "the electronic conductivity of the material",
                                  "symbol": "sigma"
                                },
                                "density" : {
                                  "$ref": "#/definitions/PhysicalQuantity",
                                  "description": "the mass density of the material",
                                  "symbol": "rho"
                                },
                                "massFraction" : {
                                  "$ref": "#/definitions/PhysicalQuantity",
                                  "description": "the ratio of the mass of the material to the total mass of the phase or mixture",
                                  "symbol": "gamma"
                                }
                              }
              }
             ],
    
    
    "required" : ["density", "massFraction"]
  }
  
